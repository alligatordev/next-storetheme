{
  "name": "artwithaliens",
  "version": "1.0.0",
  "description": "A WordPress decoupled application in React built with Next.js",
  "scripts": {
    "analyze:bundle": "ANALYZE_BUNDLE=true yarn build",
    "analyze:unused": "next-unused",
    "build": "next build",
    "dev": "concurrently \"next dev\" \"yarn generate --watch\"",
    "format": "prettier --write \"{src,types}/**/*.{ts,tsx}\" *.{js,json,md,ts}",
    "generate": "graphql-codegen",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "start": "next start",
    "storybook": "start-storybook -s ./public"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src"
    ],
    "exclude": [
      ".stories.",
      "src/service-worker.ts"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artwithaliens/storefront.git"
  },
  "keywords": [
    "decoupled",
    "react",
    "next.js",
    "graphql"
  ],
  "author": "Eike Foken <eikefoken@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artwithaliens/storefront/issues"
  },
  "homepage": "https://artwithaliens.com",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^5.0.0-alpha.14",
    "@material-ui/lab": "^5.0.0-alpha.14",
    "@material-ui/styles": "^5.0.0-alpha.14",
    "@next/bundle-analyzer": "^10.0.0",
    "@next/plugin-sentry": "^10.0.0",
    "braintree-web": "^3.68.0",
    "clsx": "^1.1.1",
    "formik": "^2.2.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "html-react-parser": "^0.14.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.20",
    "next": "^10.0.0",
    "next-offline": "^5.0.2",
    "next-seo": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-cookie-consent": "^5.2.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.2.0",
    "react-is": "^16.13.1",
    "react-number-format": "^4.4.1",
    "react-swipeable-views": "^0.13.9",
    "react-use": "^15.3.4",
    "style-to-object": "^0.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.12.3",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/fragment-matcher": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@next/eslint-plugin-next": "^10.0.0",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@storybook/theming": "^6.0.28",
    "@types/braintree-web": "^3.62.2",
    "@types/lodash": "^4.14.164",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/workbox-sw": "^4.3.1",
    "@types/yup": "^0.29.9",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-artwithaliens": "^1.7.1",
    "next-unused": "^0.0.3",
    "prettier": "^2.1.2",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-shared": "^4.4.2",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2"
  }
}
